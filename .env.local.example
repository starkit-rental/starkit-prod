NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_ENV=development

# Resend Email API - Get your API key from https://resend.com/api-keys
# Replace re_xxxxxxxxx with your real API key
RESEND_API_KEY=re_xxxxxxxxx
RESEND_AUDIENCE_ID=

# SANITY CMS
# use current date (YYYY-MM-DD) to target the latest API version
NEXT_PUBLIC_SANITY_API_VERSION=2024-10-18
NEXT_PUBLIC_SANITY_PROJECT_ID=
NEXT_PUBLIC_SANITY_DATASET=development
SANITY_API_READ_TOKEN=

# INPOST GEOWIDGET
# Token from InPost Manager (https://manager.paczkomaty.pl)
NEXT_PUBLIC_INPOST_GEOWIDGET_TOKEN=your_inpost_token_here

# Resend Email API
ADMIN_EMAIL=admin@starkit.pl